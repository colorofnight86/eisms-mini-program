.indexContainer {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #ddd;
  height: 100%;
}
.avatarUrl {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin: 100rpx 0;
}

.userName {
  font-size: 32rpx;
  margin: 100rpx 0;
}

.slogan {
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  border: 1rpx solid #333;
  border-radius: 10rpx;
}
